      "source": [
        "#!/usr/bin/env python3\n",
        "\"\"\"\n",
        "SD-Pinnokio Cloud Launcher - Repository Integrated Version\n",
        "Uses existing cloud detection and repository functions from Phase 1-12 implementation\n",
        "\"\"\"\n",
        "\n",
        "import os\n",
        "import sys\n",
        "import subprocess\n",
        "import json\n",
        "import time\n",
        "from pathlib import Path\n",
        "from typing import Optional, Dict, Any\n",
        "from IPython.display import display, HTML, clear_output\n",
        "import ipywidgets as widgets\n",
        "from ipywidgets import VBox, HBox, Button, Output, HTML as HTMLWidget\n",
        "\n",
        "# ================================================================================================\n",
        "# SECTION 1: REPOSITORY CLONING WITH BASIC CLOUD DETECTION\n",
        "# ================================================================================================\n",
        "\n",
        "def log_step(message: str, status: str = \"info\"):\n",
        "    \"\"\"Enhanced logging with colored output\"\"\"\n",
        "    colors = {\n",
        "        \"info\": \"#2196F3\",\n",
        "        \"success\": \"#4CAF50\", \n",
        "        \"warning\": \"#FF9800\",\n",
        "        \"error\": \"#F44336\",\n",
        "        \"details\": \"#9C27B0\"\n",
        "    }\n",
        "    color = colors.get(status, \"#000000\")\n",
        "    \n",
        "    display(HTML(f\"\"\"<div style=\"\n",
        "        margin: 2px 0; \n",
        "        padding: 8px 12px; \n",
        "        border-left: 4px solid {color}; \n",
        "        background-color: {'#f8f9fa' if status != 'error' else '#fff5f5'};\n",
        "        font-family: 'Segoe UI', sans-serif;\n",
        "        font-size: 13px;\n",
        "        border-radius: 0 4px 4px 0;\n",
        "    \">\n",
        "        <strong style=\"color: {color};\">[{status.upper()}]</strong> {message}\n",
        "    </div>\"\"\"))\n",
        "\n",
        "def detect_basic_environment() -> Dict[str, Any]:\n",
        "    \"\"\"Basic environment detection for initial repo cloning\"\"\"\n",
        "    \n",
        "    log_step(\"üîç Detecting cloud environment for repository setup...\", \"info\")\n",
        "    \n",
        "    # Google Colab Detection\n",
        "    try:\n",
        "        import google.colab\n",
        "        log_step(\"‚úÖ Google Colab environment detected\", \"success\")\n",
        "        return {\n",
        "            'platform': 'colab',\n",
        "            'clone_path': '/content/SD-Pinnokio',\n",
        "            'base_path': '/content'\n",
        "        }\n",
        "    except ImportError:\n",
        "        pass\n",
        "    \n",
        "    # Check for other cloud environments\n",
        "    cwd = str(Path.cwd())\n",
        "    \n",
        "    if '/workspace' in cwd:\n",
        "        if 'VAST_CONTAINER_ID' in os.environ or any('vast' in str(p) for p in [Path('/opt/vast'), Path('/vast')] if p.exists()):\n",
        "            log_step(\"‚úÖ Vast.ai environment detected\", \"success\")\n",
        "            return {\n",
        "                'platform': 'vast',\n",
        "                'clone_path': '/workspace/SD-Pinnokio',\n",
        "                'base_path': '/workspace'\n",
        "            }\n",
        "        else:\n",
        "            log_step(\"‚úÖ RunPod environment detected\", \"success\")\n",
        "            return {\n",
        "                'platform': 'runpod',\n",
        "                'clone_path': '/workspace/SD-Pinnokio',\n",
        "                'base_path': '/workspace'\n",
        "            }\n",
        "    \n",
        "    if '/teamspace' in cwd or 'LIGHTNING_CLOUD_URL' in os.environ:\n",
        "        log_step(\"‚úÖ Lightning.ai environment detected\", \"success\")\n",
        "        return {\n",
        "            'platform': 'lightning',\n",
        "            'clone_path': str(Path.cwd() / 'SD-Pinnokio'),\n",
        "            'base_path': str(Path.cwd())\n",
        "        }\n",
        "    \n",
        "    if '/notebooks' in cwd or 'PAPERSPACE_API_KEY' in os.environ:\n",
        "        log_step(\"‚úÖ Paperspace environment detected\", \"success\")\n",
        "        return {\n",
        "            'platform': 'paperspace',\n",
        "            'clone_path': '/notebooks/SD-Pinnokio',\n",
        "            'base_path': '/notebooks'\n",
        "        }\n",
        "    \n",
        "    # Local fallback\n",
        "    log_step(\"üè† Local/Unknown environment - using current directory\", \"warning\")\n",
        "    return {\n",
        "        'platform': 'local',\n",
        "        'clone_path': str(Path.cwd() / 'SD-Pinnokio'),\n",
        "        'base_path': str(Path.cwd())\n",
        "    }\n",
        "\n",
        "def clone_repository(config: Dict[str, Any]) -> bool:\n",
        "    \"\"\"Clone SD-Pinnokio repository with progress tracking\"\"\"\n",
        "    \n",
        "    clone_path = Path(config['clone_path'])\n",
        "    repo_url = \"https://github.com/remphanostar/SD-Pinnokio.git\"\n",
        "    \n",
        "    log_step(\"üöÄ Starting SD-Pinnokio repository setup...\", \"info\")\n",
        "    \n",
        "    try:\n",
        "        # Check if repository already exists\n",
        "        if clone_path.exists():\n",
        "            log_step(f\"üìÅ Repository already exists at {clone_path}\", \"warning\")\n",
        "            \n",
        "            # Verify key files exist\n",
        "            required_files = ['github_repo', 'cleaned_pinokio_apps.json']\n",
        "            all_exist = all((clone_path / f).exists() for f in required_files)\n",
        "            \n",
        "            if all_exist:\n",
        "                log_step(\"‚úÖ Repository verified - updating...\", \"success\")\n",
        "                try:\n",
        "                    os.chdir(str(clone_path))\n",
        "                    result = subprocess.run(['git', 'pull', 'origin', 'main'], \n",
        "                                           capture_output=True, text=True, timeout=30)\n",
        "                    if result.returncode == 0:\n",
        "                        log_step(\"‚úÖ Repository updated successfully\", \"success\")\n",
        "                    else:\n",
        "                        log_step(\"‚ö†Ô∏è Update had warnings, continuing...\", \"warning\")\n",
        "                    return True\n",
        "                except Exception as e:\n",
        "                    log_step(f\"‚ö†Ô∏è Update failed: {str(e)}, using existing repo\", \"warning\")\n",
        "                    return True\n",
        "            else:\n",
        "                log_step(\"üóëÔ∏è Removing incomplete repository...\", \"warning\")\n",
        "                import shutil\n",
        "                shutil.rmtree(str(clone_path), ignore_errors=True)\n",
        "        \n",
        "        # Clone fresh repository\n",
        "        log_step(f\"üì• Cloning repository from {repo_url}\", \"info\")\n",
        "        \n",
        "        # Create parent directory\n",
        "        clone_path.parent.mkdir(parents=True, exist_ok=True)\n",
        "        \n",
        "        # Clone with timeout\n",
        "        process = subprocess.Popen(\n",
        "            ['git', 'clone', '--progress', repo_url, str(clone_path)],\n",
        "            stdout=subprocess.PIPE,\n",
        "            stderr=subprocess.STDOUT,\n",
        "            universal_newlines=True,\n",
        "            bufsize=1\n",
        "        )\n",
        "        \n",
        "        # Monitor progress with timeout\n",
        "        start_time = time.time()\n",
        "        while True:\n",
        "            if time.time() - start_time > 120:  # 2 minute timeout\n",
        "                process.terminate()\n",
        "                log_step(\"‚ùå Clone timed out after 2 minutes\", \"error\")\n",
        "                return False\n",
        "                \n",
        "            output = process.stdout.readline()\n",
        "            if output == '' and process.poll() is not None:\n",
        "                break\n",
        "            if output:\n",
        "                line = output.strip()\n",
        "                if 'Cloning into' in line:\n",
        "                    log_step(f\"üì• {line}\", \"info\")\n",
        "                elif 'Receiving objects' in line or 'Resolving deltas' in line:\n",
        "                    if '%' in line:  # Only show progress lines\n",
        "                        log_step(f\"‚¨¨ {line}\", \"details\")\n",
        "                elif 'done' in line.lower():\n",
        "                    log_step(f\"‚úÖ {line}\", \"success\")\n",
        "        \n",
        "        return_code = process.poll()\n",
        "        \n",
        "        if return_code == 0:\n",
        "            log_step(\"‚úÖ Repository cloned successfully\", \"success\")\n",
        "            \n",
        "            # Verify clone\n",
        "            required_files = ['github_repo', 'cleaned_pinokio_apps.json']\n",
        "            for file in required_files:\n",
        "                if (clone_path / file).exists():\n",
        "                    log_step(f\"‚úÖ Verified: {file}\", \"success\")\n",
        "                else:\n",
        "                    log_step(f\"‚ùå Missing: {file}\", \"error\")\n",
        "                    return False\n",
        "            \n",
        "            return True\n",
        "        else:\n",
        "            log_step(f\"‚ùå Clone failed with return code {return_code}\", \"error\")\n",
        "            return False\n",
        "            \n",
        "    except Exception as e:\n",
        "        log_step(f\"‚ùå Repository setup error: {str(e)}\", \"error\")\n",
        "        return False\n",
        "\n",
        "# ================================================================================================\n",
        "# SECTION 2: REPOSITORY INTEGRATION AND SETUP\n",
        "# ================================================================================================\n",
        "\n",
        "def setup_repository_integration(clone_path: Path) -> bool:\n",
        "    \"\"\"Setup Python path and import repository modules\"\"\"\n",
        "    \n",
        "    log_step(\"üêç Setting up repository integration...\", \"info\")\n",
        "    \n",
        "    try:\n",
        "        # Add paths to Python path\n",
        "        paths_to_add = [\n",
        "            str(clone_path),\n",
        "            str(clone_path / 'github_repo')\n",
        "        ]\n",
        "        \n",
        "        for path in paths_to_add:\n",
        "            if path not in sys.path:\n",
        "                sys.path.insert(0, path)\n",
        "                log_step(f\"üìÅ Added to Python path: {path}\", \"details\")\n",
        "        \n",
        "        # Change to repository directory\n",
        "        os.chdir(str(clone_path))\n",
        "        log_step(f\"üìÅ Changed working directory to: {clone_path}\", \"details\")\n",
        "        \n",
        "        return True\n",
        "        \n",
        "    except Exception as e:\n",
        "        log_step(f\"‚ùå Repository integration failed: {str(e)}\", \"error\")\n",
        "        return False\n",
        "\n",
        "def install_core_dependencies() -> bool:\n",
        "    \"\"\"Install essential dependencies for SD-Pinnokio\"\"\"\n",
        "    \n",
        "    log_step(\"üì¶ Installing core dependencies...\", \"info\")\n",
        "    \n",
        "    # Essential packages for basic functionality\n",
        "    core_packages = [\n",
        "        'ipywidgets>=8.0.0',\n",
        "        'requests>=2.31.0',\n",
        "        'tqdm>=4.65.0'\n",
        "    ]\n",
        "    \n",
        "    try:\n",
        "        for package in core_packages:\n",
        "            log_step(f\"üì¶ Installing {package}...\", \"details\")\n",
        "            result = subprocess.run(\n",
        "                [sys.executable, '-m', 'pip', 'install', '-q', package],\n",
        "                capture_output=True, text=True, timeout=60\n",
        "            )\n",
        "            \n",
        "            if result.returncode == 0:\n",
        "                log_step(f\"‚úÖ Installed {package}\", \"success\")\n",
        "            else:\n",
        "                log_step(f\"‚ö†Ô∏è Warning installing {package}: {result.stderr[:100]}\", \"warning\")\n",
        "        \n",
        "        return True\n",
        "        \n",
        "    except Exception as e:\n",
        "        log_step(f\"‚ö†Ô∏è Dependency installation had issues: {str(e)}\", \"warning\")\n",
        "        return True  # Continue even with warnings\n",
        "\n",
        "# ================================================================================================\n",
        "# SECTION 3: REPOSITORY-BASED SYSTEM INITIALIZATION\n",
        "# ================================================================================================\n",
        "\n",
        "def initialize_sd_pinnokio_system(clone_path: Path) -> tuple:\n",
        "    \"\"\"Initialize SD-Pinnokio system using repository modules\"\"\"\n",
        "    \n",
        "    log_step(\"üöÄ Initializing SD-Pinnokio system using repository modules...\", \"info\")\n",
        "    \n",
        "    try:\n",
        "        # Import repository modules\n",
        "        log_step(\"üìã Importing cloud detection module...\", \"info\")\n",
        "        \n",
        "        # Try to import cloud detection from repository\n",
        "        cloud_detector = None\n",
        "        environment_manager = None\n",
        "        app_manager = None\n",
        "        \n",
        "        try:\n",
        "            from cloud_detection.cloud_detector import CloudDetector\n",
        "            cloud_detector = CloudDetector()\n",
        "            log_step(\"‚úÖ CloudDetector imported successfully\", \"success\")\n",
        "        except ImportError as e:\n",
        "            log_step(f\"‚ö†Ô∏è CloudDetector not available: {str(e)}\", \"warning\")\n",
        "        \n",
        "        try:\n",
        "            from environment_management.environment_manager import EnvironmentManager\n",
        "            environment_manager = EnvironmentManager()\n",
        "            log_step(\"‚úÖ EnvironmentManager imported successfully\", \"success\")\n",
        "        except ImportError as e:\n",
        "            log_step(f\"‚ö†Ô∏è EnvironmentManager not available: {str(e)}\", \"warning\")\n",
        "        \n",
        "        try:\n",
        "            from app_manager import AppManager\n",
        "            app_manager = AppManager()\n",
        "            log_step(\"‚úÖ AppManager imported successfully\", \"success\")\n",
        "        except ImportError as e:\n",
        "            log_step(f\"‚ö†Ô∏è AppManager not available: {str(e)}\", \"warning\")\n",
        "        \n",
        "        # Load application database\n",
        "        apps_data = {}\n",
        "        try:\n",
        "            apps_file = clone_path / 'cleaned_pinokio_apps.json'\n",
        "            if apps_file.exists():\n",
        "                with open(apps_file, 'r', encoding='utf-8') as f:\n",
        "                    apps_data = json.load(f)\n",
        "                log_step(f\"‚úÖ Loaded {len(apps_data)} applications from database\", \"success\")\n",
        "                \n",
        "                # Show categories\n",
        "                categories = {}\n",
        "                for app_data in apps_data.values():\n",
        "                    category = app_data.get('category', 'UNKNOWN')\n",
        "                    categories[category] = categories.get(category, 0) + 1\n",
        "                \n",
        "                log_step(\"üìä Application categories:\", \"info\")\n",
        "                for category, count in sorted(categories.items()):\n",
        "                    log_step(f\"   ‚Ä¢ {category}: {count} apps\", \"details\")\n",
        "        except Exception as e:\n",
        "            log_step(f\"‚ö†Ô∏è Could not load application database: {str(e)}\", \"warning\")\n",
        "        \n",
        "        return cloud_detector, environment_manager, app_manager, apps_data\n",
        "        \n",
        "    except Exception as e:\n",
        "        log_step(f\"‚ùå System initialization error: {str(e)}\", \"error\")\n",
        "        return None, None, None, {}\n",
        "\n",
        "# ================================================================================================\n",
        "# SECTION 4: ENHANCED UI USING REPOSITORY FUNCTIONS\n",
        "# ================================================================================================\n",
        "\n",
        "def create_sd_pinnokio_interface(clone_path: Path, cloud_detector, app_manager, apps_data):\n",
        "    \"\"\"Create SD-Pinnokio interface using repository-based components\"\"\"\n",
        "    \n",
        "    # Get platform info\n",
        "    platform = \"unknown\"\n",
        "    if cloud_detector:\n",
        "        try:\n",
        "            platform_info = cloud_detector.detect_environment()\n",
        "            platform = platform_info.get('platform', 'unknown')\n",
        "        except:\n",
        "            pass\n",
        "    \n",
        "    # Header\n",
        "    header_html = f\"\"\"\n",
        "    <div style=\"\n",
        "        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n",
        "        color: white;\n",
        "        padding: 25px;\n",
        "        border-radius: 15px;\n",
        "        margin-bottom: 25px;\n",

        "        <p style=\"margin: 10px 0 5px 0; opacity: 0.9; font-size: 18px;\">\n",
        "            Multi-Cloud AI Application Manager\n",
        "        </p>\n",
        "        <p style=\"margin: 5px 0 0 0; opacity: 0.8; font-size: 16px;\">\n",
        "            Platform: <strong>{platform.upper()}</strong> ‚Ä¢ \n",
        "            {len(apps_data)} Applications Available\n",
        "        </p>\n",
        "    </div>\n",
        "    \"\"\"\n",
        "    \n",
        "    display(HTML(header_html))\n",
        "    \n",
        "    # Success message\n",
        "    success_html = \"\"\"\n",
        "    <div style=\"\n",
        "        background: linear-gradient(135deg, #4CAF50 0%, #45a049 100%);\n",
        "        color: white;\n",
        "        padding: 20px;\n",
        "        border-radius: 10px;\n",
        "        margin: 20px 0;\n",
        "        text-align: center;\n",
        "        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n",
        "    \">\n",
        "        <h2 style=\"margin: 0 0 10px 0; font-size: 24px;\">üéâ SD-Pinnokio Ready!</h2>\n",
        "        <p style=\"margin: 0; opacity: 0.9; font-size: 16px;\">\n",
        "            Repository cloned, modules imported, and system initialized successfully.\n",
        "        </p>\n",
        "    </div>\n",
        "    \"\"\"\n",
        "    \n",
        "    display(HTML(success_html))\n",
        "    \n",
        "    # Application statistics if available\n",
        "    if apps_data:\n",
        "        categories = {}\n",
        "        for app_data in apps_data.values():\n",
        "            category = app_data.get('category', 'UNKNOWN')\n",
        "            categories[category] = categories.get(category, 0) + 1\n",
        "        \n",
        "        stats_html = \"\"\"\n",
        "        <div style=\"margin: 20px 0;\">\n",
        "            <h3 style=\"color: #2196F3; margin-bottom: 15px;\">üìä Application Database</h3>\n",
        "            <div style=\"display: grid; grid-template-columns: repeat(auto-fit, minmax(120px, 1fr)); gap: 10px;\">\n",
        "        \"\"\"\n",
        "        \n",
        "        category_colors = {\n",
        "            'IMAGE': '#FF6B6B', 'VIDEO': '#4ECDC4', 'AUDIO': '#45B7D1',\n",
        "            'TEXT': '#96CEB4', 'LLM': '#FFEAA7', 'UTILITY': '#DDA0DD',\n",
        "            'UNKNOWN': '#95A5A6'\n",
        "        }\n",
        "        \n",
        "        for category, count in sorted(categories.items()):\n",
        "            color = category_colors.get(category, '#95A5A6')\n",
        "            stats_html += f\"\"\"\n",
        "            <div style=\"\n",
        "                background: {color};\n",
        "                color: white;\n",
        "                padding: 15px;\n",
        "                border-radius: 8px;\n",
        "                text-align: center;\n",
        "            \">\n",
        "                <strong style=\"font-size: 18px;\">{count}</strong><br>\n",
        "                <small>{category}</small>\n",
        "            </div>\n",
        "            \"\"\"\n",
        "        \n",
        "        stats_html += \"\"\"\n",
        "            </div>\n",
        "        </div>\n",
        "        \"\"\"\n",
        "        \n",
        "        display(HTML(stats_html))\n",
        "    \n",
        "    # Action buttons\n",
        "    launch_streamlit_btn = Button(\n",
        "        description=\"üåê Launch Streamlit UI\",\n",
        "        button_style='primary',\n",
        "        layout={'width': '200px', 'height': '40px'}\n",
        "    )\n",
        
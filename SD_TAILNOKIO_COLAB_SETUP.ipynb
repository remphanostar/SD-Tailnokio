    "# @title üîÑ Update Repository to Latest Version\n",
    "# This cell pulls the latest version and overwrites existing files\n",
    "\n",
    "import os\n",
    "import subprocess\n",
    "import sys\n",
    "from IPython.display import clear_output, HTML, display\n",
    "\n",
    "REPO_DIR = \"/content/SD-Tailnokio\"\n",
    "REPO_URL = \"https://github.com/remphanostar/SD-Tailnokio.git\"\n",
    "\n",
    "print(\"üîÑ Updating SD-Tailnokio to latest version...\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "def run_command(cmd, description=\"\"):\n",
    "    if description:\n",
    "        print(f\"\\nüìã {description}\")\n",
    "    print(f\"üîß Running: {cmd}\")\n",
    "    \n",
    "    result = subprocess.run(cmd, shell=True, capture_output=True, text=True)\n",
    "    \n",
    "    if result.stdout:\n",
    "        print(\"‚úÖ Output:\")\n",
    "        print(result.stdout)\n",
    "    \n",
    "    if result.stderr:\n",
    "        print(\"‚ùå Errors:\")\n",
    "        print(result.stderr)\n",
    "    \n",
    "    return result.returncode == 0\n",
    "\n",
    "# Check if repository exists\n",
    "if os.path.exists(REPO_DIR):\n",
    "    print(\"üìÅ Repository exists, updating...\")\n",
    "    os.chdir(REPO_DIR)\n",
    "    \n",
    "    # Stash any local changes\n",
    "    print(\"üíæ Stashing local changes...\")\n",
    "    subprocess.run([\"git\", \"stash\"], check=True)\n",
    "    \n",
    "    # Pull latest changes\n",
    "    print(\"üì• Pulling latest changes...\")\n",
    "    success = run_command(\"git pull origin master\", \"Pulling latest changes\")\n",
    "    \n",
    "    if success:\n",
    "        print(\"‚úÖ Repository updated successfully!\")\n",
    "    else:\n",
    "        print(\"‚ùå Failed to update repository\")\n",
    "else:\n",
    "    print(\"üìÅ Repository not found, cloning...\")\n",
    "    success = run_command(f\"git clone {REPO_URL} {REPO_DIR}\", \"Cloning repository\")\n",
    "    \n",
    "    if success:\n",
    "        os.chdir(REPO_DIR)\n",
    "        print(\"‚úÖ Repository cloned successfully!\")\n",
    "    else:\n",
    "        print(\"‚ùå Failed to clone repository\")\n",
    "        sys.exit(1)\n",
    "\n",
n",
    "# Install/update dependencies\n",
    "print(\"\\nüì¶ Installing/updating dependencies...\")\n",
    "success = run_command(f\"{sys.executable} -m pip install -r requirements.txt\", \"Installing dependencies\")\n",
    "\n",
    "if success:\n",
    "    print(\"‚úÖ Dependencies updated successfully!\")\n",
    "else:\n",
    "    print(\"‚ö†Ô∏è Some dependencies may have failed to install\")\n",
    "\n",
    "# Run tests\n",
    "print(\"\\nüß™ Running integration tests...\")\n",
    "success = run_command(f\"{sys.executable} test-integration.py\", \"Running tests\")\n",
    "\n",
    "if success:\n",
    "    print(\"‚úÖ All tests passed!\")\n",
    "else:\n",
    "    print(\"‚ö†Ô∏è Some tests failed\")\n",
    "\n",
    "print(\"\\n\" + \"=\" * 50)\n",
    "print(\"üîÑ Update complete!\")\n",
    "print(\"üìç Current directory:\", os.getcwd())\n",
    "print(\"üöÄ Ready to use! Choose a launch option above.\")\n",
    "print(\"=\" * 50)"
   ]

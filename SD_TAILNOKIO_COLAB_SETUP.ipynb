   },
   "outputs": [],
   "source": [
    "# @title üîß Step 1: Clone Repository and Setup Dependencies\n",
    "\n",
    "import os\n",
    "import subprocess\n",
    "import sys\n",
    "from IPython.display import clear_output, HTML, display\n",
    "import time\n",
    "\n",
    "print(\"üöÄ Setting up SD-Tailnokio in Colab...\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "# Repository information\n",
    "REPO_URL = \"https://github.com/remphanostar/SD-Tailnokio.git\"\n",
    "REPO_DIR = \"/content/SD-Tailnokio\"\n",
    "\n",
    "# Function to run commands and display output\n",
    "def run_command(cmd, description=\"\"):\n",
    "    if description:\n",
    "        print(f\"\\nüìã {description}\")\n",
    "    print(f\"üîß Running: {cmd}\")\n",
    "    \n",
    "    result = subprocess.run(cmd, shell=True, capture_output=True, text=True)\n",
    "    \n",
    "    if result.stdout:\n",
    "        print(\"‚úÖ Output:\")\n",
    "        print(result.stdout)\n",
    "    \n",
    "    if result.stderr:\n",
    "        print(\"‚ùå Errors:\")\n",
    "        print(result.stderr)\n",
    "    \n",
    "    return result.returncode == 0\n",
    "\n",
    "# Step 1: Remove existing repository if it exists\n",
    "if os.path.exists(REPO_DIR):\n",
    "    print(\"üóëÔ∏è Removing existing repository...\")\n",
    "    subprocess.run([\"rm\", \"-rf\", REPO_DIR], check=True)\n",
    "    print(\"‚úÖ Existing repository removed\")\n",
    "\n",
    "# Step 2: Clone the repository\n",
    "print(\"\\nüì• Cloning SD-Tailnokio repository...\")\n",
    "success = run_command(f\"git clone {REPO_URL} {REPO_DIR}\", \"Cloning repository\")\n",
    "\n",
    "if not success:\n",
    "    print(\"‚ùå Failed to clone repository\")\n",
    "    sys.exit(1)\n",
    "\n",
    "# Step 3: Change to repository directory\n",
    "os.chdir(REPO_DIR)\n",
    "print(f\"\\nüìÅ Changed to directory: {os.getcwd()}\")\n",
    "\n",
    "# Step 4: Install Python dependencies\n",
    "print(\"\\nüì¶ Installing Python dependencies...\")\n",
    "success = run_command(f\"{sys.executable} -m pip install -r requirements.txt\", \"Installing Python packages\")\n",
    "\n",
    "if not success:\n",
    "    print(\"‚ùå Failed to install Python dependencies\")\n",
    "    sys.exit(1)\n",
    "\n",
    "# Step 5: Install additional dependencies for web interface\n",
    "additional_deps = [\n",
    "    \"flask\",\n",
    "    \"flask-cors\",\n",
    "    \"requests\",\n",
    "    \"qrcode[pil]\",\n",
    "    \"pyngrok\",\n",
    "    \"psutil\",\n",
    "    \"ipywidgets\"\n",
    "]\n",
    "\n",
    "print(\"\\nüì¶ Installing additional dependencies...\")\n",
    "for dep in additional_deps:\n",
    "    run_command(f\"{sys.executable} -m pip install {dep}\", f\"Installing {dep}\")\n",
    "\n",
    "# Step 6: Test the setup\n",
    "print(\"\\nüß™ Testing setup...\")\n",
    "success = run_command(f\"{sys.executable} test-integration.py\", \"Running integration tests\")\n",
    "\n",
    "if success:\n",
    "    print(\"\\n‚úÖ Setup completed successfully!\")\n",
    "    print(\"üéâ SD-Tailnokio is ready to use!\")\n",
    "else:\n",
    "    print(\"\\n‚ö†Ô∏è Some tests failed, but continuing...\")\n",
    "\n",
    "print(\"\\n\" + \"=\" * 50)\n",
    "print(\"üöÄ Setup complete! Choose a launch option below.\")\n",
    "print(\"=\" * 50)"
   ]

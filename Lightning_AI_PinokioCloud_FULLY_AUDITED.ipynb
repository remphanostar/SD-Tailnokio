   "source": [
    "# üîß CELL 4: REPOSITORY MODULE IMPORTS (VERIFIED)\n",
    "\n",
    "if clone_success:\n",
    "    print(\"üîß IMPORTING VERIFIED REPOSITORY MODULES\")\n",
    "    \n",
    "    verified_modules = {}\n",
    "    \n",
    "    # Test each import individually\n",
    "    import_tests = [\n",
    "        ('cloud_detection.cloud_detector', 'CloudDetector'),\n",
    "        ('environment_management.venv_manager', 'VirtualEnvironmentManager'),\n",
    "        ('app_analysis.app_analyzer', 'AppAnalyzer'),\n",
    "        ('platforms.lightning_optimizer', 'LightningOptimizer')\n",
    "    ]\n",
    "    \n",
    "    for module_name, class_name in import_tests:\n",
    "        try:\n",
    "            module = __import__(module_name, fromlist=[class_name])\n",
    "            cls = getattr(module, class_name, None)\n",
    "            if cls:\n",
    "                verified_modules[module_name] = cls\n",
    "                print(f\"  ‚úÖ {module_name}.{class_name}\")\n",
    "        except ImportError as e:\n",
    "            print(f\"  ‚ùå {module_name}: Import failed - {e}\")\n",
    "        except AttributeError:\n",
    "            print(f\"  ‚ùå {module_name}: Class {class_name} not found\")\n",
    "        except Exception as e:\n",
    "            print(f\"  ‚ö†Ô∏è {module_name}: Other error - {e}\")\n",
    "    \n",
    "    print(f\"\n‚úÖ {len(verified_modules)} modules successfully imported\")\n",
    "    modules_loaded = len(verified_modules) > 0\n",
    "    \n",
    "else:\n",
    "    print(\"‚ùå Cannot import modules - repository not available\")\n",
    "    verified_modules = {}\n",
    "    modules_loaded = False"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,

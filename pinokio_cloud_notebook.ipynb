    "#@title **Cell 1: Environment Setup & Repository Cloning**\n",
    "\"\"\"\n",
    "PinokioCloud Environment Setup\n",
    "Automatic cloud detection and repository cloning with all 12 phases\n",
    "\"\"\"\n",
    "\n",
    import os\n",
    import sys\n",
    import json\n",
    import subprocess\n",
    import shutil\n",
    from pathlib import Path\n",
    \n",
    "print(\"üöÄ PinokioCloud Environment Setup\")\n",
    print(\"=\" * 50)\n",
    "\n",
    "# Step 1: Detect cloud platform\n",
    "def detect_cloud_platform():\n",
    "    \"\"\"Detect the current cloud platform with intelligent analysis.\"\"\"\n",
    "    \n",
    "    # Check environment variables and paths\n",
    "    env_indicators = {\n",
    "        'Google Colab': [\n",
    "            '/content/',\n",
    "            'COLAB_GPU',\n",
    "            'COLAB_TPU_ADDR'\n",
    "        ],\n",
    "        'Vast.ai': [\n",
    "            '/workspace/',\n",
    "            'VAST_CONTAINER_ID',\n",
    "            'VAST_SERVER_ID'\n",
    "        ],\n",
    "        'Lightning.ai': [\n",
    "            '/teamspace/',\n",
    "            'LIGHTNING_TEAM_ID',\n",
    "            'LIGHTNING_PROJECT_ID'\n",
    "        ],\n",
    "        'Paperspace': [\n",
    "            '/notebooks/',\n",
    "            'PAPERSPACE_NOTEBOOK_ID',\n",
    "            'PAPERSPACE_PROJECT_ID'\n",
    "        ],\n",
    "        'RunPod': [\n",
    "            '/runpod/',\n",
    "            'RUNPOD_POD_ID',\n",
    "            'RUNPOD_PROJECT_ID'\n",
    "        ]\n",
    "    }\n",
    "    \n",
    "    detected_platforms = []\n",
    "    \n",
    "    for platform, indicators in env_indicators.items():\n",
    "        for indicator in indicators:\n",
    "            if indicator in os.environ.get('PWD', '') or os.environ.get(indicator, ''):\n",
    "                detected_platforms.append(platform)\n",
    "                break\n",
    "            if os.path.exists(indicator):\n",
    "                detected_platforms.append(platform)\n",
    "                break\n",
    "    \n",
    "    if detected_platforms:\n",
    "        platform = detected_platforms[0]\n",
    "        print(f\"‚úÖ Detected platform: {platform}\")\n",
    "        return platform\n",
    "    else:\n",
    "        print(\"‚ö†Ô∏è Unknown platform, using default configuration\")\n",
    "        return \"Unknown\"\n",
    "\n",
    "# Step 2: Get platform-specific paths\n",
    "def get_platform_paths(platform):\n",
    "    \"\"\"Get platform-specific paths and configurations.\"\"\"\n",
    "    \n",

    "            'apps_path': '/content/pinokio-cloud/apps/',\n",
    "            'venv_path': '/content/pinokio-cloud/venvs/'\n",
    "        },\n",
    "        'Vast.ai': {\n",
    "            'base_path': '/workspace/',\n",
    "            'repo_path': '/workspace/pinokio-cloud/',\n",
    "            'apps_path': '/workspace/pinokio-cloud/apps/',\n",
    "            'venv_path': '/workspace/pinokio-cloud/venvs/'\n",
    "        },\n",
    "        'Lightning.ai': {\n",
    "            'base_path': '/teamspace/studios/this_studio/',\n",
    "            'repo_path': '/teamspace/studios/this_studio/pinokio-cloud/',\n",
    "            'apps_path': '/teamspace/studios/this_studio/pinokio-cloud/apps/',\n",
    "            'venv_path': '/teamspace/studios/this_studio/pinokio-cloud/venvs/'\n",
    "        },\n",
    "        'Paperspace': {\n",
    "            'base_path': '/notebooks/',\n",
    "            'repo_path': '/notebooks/pinokio-cloud/',\n",
    "            'apps_path': '/notebooks/pinokio-cloud/apps/',\n",
    "            'venv_path': '/notebooks/pinokio-cloud/venvs/'\n",
    "        },\n",
    "        'RunPod': {\n",
    "            'base_path': '/runpod/',\n",
    "            'repo_path': '/runpod/pinokio-cloud/',\n",
    "            'apps_path': '/runpod/pinokio-cloud/apps/',\n",
    "            'venv_path': '/runpod/pinokio-cloud/venvs/'\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    return paths.get(platform, paths['Google Colab'])\n",
    "\n",
    "# Step 3: Install required dependencies\n",
    "def install_dependencies():\n",
    "    \"\"\"Install all required dependencies for PinokioCloud.\"\"\"\n",
    "    \n",
    "    print(\"üì¶ Installing required dependencies...\")\n",
    "    \n",
    "    # Essential packages\n",
    "    essential_packages = [\n",
    "        'ipywidgets',\n",
    "        'psutil',\n",
    "        'requests',\n",
    "        'gitpython',\n",
    "        'tqdm'\n",
    "    ]\n",
    "    \n",
    "    for package in essential_packages:\n",
    "        try:\n",
    "            print(f\"  Installing {package}...\")\n",
    "            result = subprocess.run(\n",
    "                [sys.executable, '-m', 'pip', 'install', package, '--quiet'],\n",
    "                capture_output=True,\n",
    "                text=True\n",
    "            )\n",
    "            if result.returncode == 0:\n",
    "                print(f\"    ‚úÖ {package} installed successfully\")\n",
    "            else:\n",
    "                print(f\"    ‚ùå Failed to install {package}: {result.stderr}\")\n",
    "        except Exception as e:\n",
    "            print(f\"    ‚ùå Error installing {package}: {e}\")\n",
    "    \n",
    "    print(\"‚úÖ Dependencies installation completed\")\n",
    "\n",
    "# Step 4: Clone repository\n",
    "def clone_repository(repo_path, platform):\n",
    "    \"\"\"Clone the SD-Pinnokio repository with progress tracking.\"\"\"\n",
    "    \n",
    "    print(\"üì• Cloning SD-Pinnokio repository...\")\n",
    "    \n",
    "    if os.path.exists(repo_path):\n",
    "        print(f\"‚ö†Ô∏è Repository already exists at {repo_path}\")\n",
    "        return True\n",
    "    \n",
    "    try:\n",
    "        # Clone repository\n",
    "        result = subprocess.run(\n",
    "            ['git', 'clone', 'https://github.com/remphanostar/SD-Pinnokio.git', repo_path],\n",
    "            capture_output=True,\n",
    "            text=True\n",
    "        )\n",
    "        \n",
    "        if result.returncode == 0:\n",
    "            print(f\"‚úÖ Repository cloned successfully to {repo_path}\")\n",
    "            return True\n",
    "        else:\n",
    "            print(f\"‚ùå Failed to clone repository: {result.stderr}\")\n",
    "            return False\n",
    "            \n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Error cloning repository: {e}\")\n",
    "        return False\n",
    "\n",
    "# Step 5: Setup directory structure\n",
    "def setup_directory_structure(paths):\n",
    "    \"\"\"Create necessary directory structure.\"\"\"\n",
    "    \n",
    "    print(\"üìÅ Setting up directory structure...\")\n",
    "    \n",
    "    directories = [\n",
    "        paths['apps_path'],\n",
    "        paths['venv_path'],\n",
    "        os.path.join(paths['repo_path'], 'logs'),\n",
    "        os.path.join(paths['repo_path'], 'cache')\n",
    "    ]\n",
    "    \n",
    "    for directory in directories:\n",
    "        try:\n",
    "            os.makedirs(directory, exist_ok=True)\n",
    "            print(f\"  ‚úÖ Created: {directory}\")\n",
    "        except Exception as e:\n",
    "            print(f\"  ‚ùå Failed to create {directory}: {e}\")\n",
    "    \n",
    "    print(\"‚úÖ Directory structure setup completed\")\n",
    "\n",
    "# Step 6: Verify repository structure\n",
    "def verify_repository_structure(repo_path):\n",
    "    \"\"\"Verify that all required phases are present.\"\"\"\n",
    "    \n",
    "    print(\"üîç Verifying repository structure...\")\n",
    "    \n",
    "    required_phases = [\n",
    "        'cloud_detection',\n",
    "        'environment_management',\n",
    "        'app_analysis',\n",
    "        'dependencies',\n",
    "        'engine',\n",
    "        'running',\n",
    "        'tunneling',\n",
    "        'platforms',\n",
    "        'optimization',\n",
    "        'testing',\n",
    "        'ui_core',\n",
    "        'ui_enhanced',\n",
    "        'finalization'\n",
    "    ]\n",
    "    \n",
    "    github_repo_path = os.path.join(repo_path, 'github_repo')\n",
    "    \n",
    "    missing_phases = []\n",
    "    for phase in required_phases:\n",
    "        phase_path = os.path.join(github_repo_path, phase)\n",
    "        if not os.path.exists(phase_path):\n",
    "            missing_phases.append(phase)\n",
    "    \n",
    "    if missing_phases:\n",
    "        print(f\"‚ùå Missing phases: {missing_phases}\")\n",
    "        return False\n",
    "    else:\n",
    "        print(f\"‚úÖ All {len(required_phases)} phases verified\")\n",
    "        return True\n",
    "\n",
    "# Main execution\n",
    "def main_setup():\n",
    "    \"\"\"Main setup function.\"\"\"\n",
    "    \n",
    "    # Detect platform\n",
    "    platform = detect_cloud_platform()\n",
    "    \n",
    "    # Get platform paths\n",
    "    paths = get_platform_paths(platform)\n",
    "    \n",
    "    print(f\"üìç Platform paths:\")\n",
    "    for key, value in paths.items():\n",
    "        print(f\"  {key}: {value}\")\n",
    "    \n",
    "    # Install dependencies\n",
    "    install_dependencies()\n",
    "    \n",
    "    # Clone repository\n",
    "    if not clone_repository(paths['repo_path'], platform):\n",
    "        print(\"‚ùå Setup failed - repository cloning error\")\n",
    "        return False\n",
    "    \n",
    "    # Setup directory structure\n",
    "    setup_directory_structure(paths)\n",
    "    \n",
    "    # Verify repository structure\n",
    "    if not verify_repository_structure(paths['repo_path']):\n",
    "        print(\"‚ùå Setup failed - repository structure verification error\")\n",
    "        return False\n",
    "    \n",
    "    print(\"\\nüéâ Setup completed successfully!\")\n",
    "    print(\"=\" * 50)\n",
    "    print(f\"‚úÖ Platform: {platform}\")\n",
    "    print(f\"‚úÖ Repository: {paths['repo_path']}\")\n",
    "    print(f\"‚úÖ All 12 phases verified\")\n",
    "    print(f\"‚úÖ Ready to launch interface\")\n",
    "    \n",
    "    # Store paths for next cell\n",
    "    with open('/tmp/pinokio_paths.json', 'w') as f:\n",
    "        json.dump({\n",
    "            'platform': platform,\n",
    "            'paths': paths,\n",
    "            'repo_path': paths['repo_path']\n",
    "        }, f)\n",
    "    \n",
    "    return True\n",
    "\n",
    "# Execute setup\n",
    "if __name__ == \"__main__\":\n",
    "    success = main_setup()\n",
    "    if success:\n",
    "        print(\"\\nüöÄ Run Cell 2 to launch the PinokioCloud interface!\")\n",
    "    else:\n",
    "        print(\"\\n‚ùå Setup failed. Please check the error messages above.\")"

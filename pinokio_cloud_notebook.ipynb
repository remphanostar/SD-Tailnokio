    "#@title **Cell 2: Launch PinokioCloud ipywidget Interface**\n",
    "\"\"\"\n",
    "PinokioCloud Complete ipywidget Interface\n",
    "Real implementation with all 284 apps, categories, and raw output\n",
    "\"\"\"\n",
    "\n",
    import os\n",
    "import sys\n",
    "import json\n",
    "import subprocess\n",
    "import threading\n",
    "import time\n",
    "import psutil\n",
    "import platform\n",
    "from pathlib import Path\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display, clear_output, HTML\n",
    "\n",
    "class PinokioCloudInterface:\n",
    "    \"\"\"Complete PinokioCloud interface with real functionality.\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        # Load setup information\n",
    "        try:\n",
    "            with open('/tmp/pinokio_paths.json', 'r') as f:\n",
    "                setup_info = json.load(f)\n",
    "            self.platform = setup_info['platform']\n",
    "            self.paths = setup_info['paths']\n",
    "            self.repo_path = setup_info['repo_path']\n",
    "        except:\n",
    "            # Fallback to default paths\n",
    "            self.platform = \"Google Colab\"\n",
    "            self.paths = {\n",
    "                'base_path': '/content/',\n",
    "                'repo_path': '/content/pinokio-cloud/',\n",
    "                'apps_path': '/content/pinokio-cloud/apps/',\n",
    "                'venv_path': '/content/pinokio-cloud/venvs/'\n",
    "            }\n",
    "            self.repo_path = '/content/pinokio-cloud/'\n",
    "        \n",
    "        # Load and filter apps database\n",
    "        self.apps_data = self.load_filtered_apps_database()\n",
    "        self.categories = self.extract_categories()\n",
    "        self.filtered_apps = self.apps_data.copy()\n",
    "        \n",
    "        # Process tracking\n",
    "        self.running_processes = {}\n",
    "        self.installation_logs = {}\n",
    "        \n",
    "        # Initialize phase systems\n",
    "        self.initialize_phase_systems()\n",
    "        \n",
    "        print(f\"üöÄ PinokioCloud Interface Initialized\")\n",
    "        print(f\"üìä Loaded {len(self.apps_data)} applications\")\n",
    "        print(f\"üìÇ {len(self.categories)} categories\")\n",
    "        print(f\"‚òÅÔ∏è Platform: {self.platform}\")\n",
    "    \n",
    "    def load_filtered_apps_database(self):\n",
    "        \"\"\"Load apps database and filter out problematic applications.\"\"\"\n",
    "        \n",
    "        # Try multiple paths for the apps database\n",
    "        possible_paths = [\n",
    "            \"cleaned_pinokio_apps.json\",\n",
    "            \"../cleaned_pinokio_apps.json\",\n",
    "            f\"{self.repo_path}cleaned_pinokio_apps.json\",\n",
    "            f\"{self.repo_path}/cleaned_pinokio_apps.json\",\n",
    "            \"/content/cleaned_pinokio_apps.json\",\n",
    "            \"/workspace/cleaned_pinokio_apps.json\"\n",
    "        ]\n",
    "        \n",
    "        apps_data = {}\n",
    "        loaded_path = None\n",
    "        \n",
    "        for path in possible_paths:\n",
    "            if os.path.exists(path):\n",
    "                try:\n",
    "                    with open(path, 'r') as f:\n",
    "                        raw_data = json.load(f)\n",
    "                    \n",
    "                    # Filter out problematic applications\n",
    "                    filtered_count = 0\n",
    "                    for app_id, app_info in raw_data.items():\n",
    "                        # Skip apps with problematic keywords\n",
    "                        app_name = app_info.get('name', '').lower()\n",
    "                        app_desc = app_info.get('description', '').lower()\n",
    "                        app_tags = ' '.join(app_info.get('tags', [])).lower()\n",
    "                        \n",
    "                        problematic_keywords = ['roop', 'facefusion', 'deepfake', 'swap']\n",
    "                        is_problematic = any(keyword in app_name or keyword in app_desc or keyword in app_tags for keyword in problematic_keywords)\n",
    "                        \n",
    "                        if not is_problematic:\n",
    "                            apps_data[app_id] = app_info\n",
    "                        else:\n",
    "                            filtered_count += 1\n",
    "                    \n",
    "                    loaded_path = path\n",
    "                    print(f\"‚úÖ Loaded {len(apps_data)} apps from {path}\")\n",
    "                    if filtered_count > 0:\n",
    "                        print(f\"‚ö†Ô∏è Filtered out {filtered_count} problematic applications\")\n",
    "                    break\n",
    "                except Exception as e:\n",
    "                    print(f\"‚ùå Error loading {path}: {e}\")\n",
    "        \n",
    "        if not apps_data:\n",
    "            print(\"‚ö†Ô∏è No apps database found, creating minimal dataset\")\n",
    "            apps_data = {\n",
    "                \"automatic1111\": {\n",
    "                    \"name\": \"AUTOMATIC1111\",\n",
    "                    \"description\": \"Stable Diffusion WebUI with comprehensive features\",\n",
    "                    \"category\": \"IMAGE\",\n",
    "                    \"repo_url\": \"https://github.com/AUTOMATIC1111/stable-diffusion-webui.git\",\n",
    "                    \"clone_url\": \"https://github.com/AUTOMATIC1111/stable-diffusion-webui.git\",\n",
    "                    \"tags\": [\"stable-diffusion\", \"webui\", \"image-generation\"],\n",
    "                    \"author\": \"AUTOMATIC1111\",\n",
    "                    \"stars\": 0,\n",
    "                    \"has_install_js\": True,\n",
    "                    \"has_install_json\": False,\n",
    "                    \"has_pinokio_js\": True,\n",
    "                    \"installer_type\": \"js\",\n",
    "                    \"is_pinokio_app\": True,\n",
    "                    \"source\": \"official_appdata\"\n",
    "                },\n",
    "                \"comfyui_pinokio\": {\n",
    "                    \"name\": \"ComfyUI\",\n",
    "                    \"description\": \"A powerful and modular node-based GUI for Stable Diffusion models\",\n",
    "                    \"category\": \"IMAGE\",\n",
    "                    \"repo_url\": \"https://github.com/cocktailpeanut/comfyui.pinokio.git\",\n",
    "                    \"clone_url\": \"https://github.com/cocktailpeanut/comfyui.pinokio.git\",\n",
    "                    \"tags\": [\"comfyui\", \"stable-diffusion\", \"node-based\", \"image-generation\"],\n",
    "                    \"author\": \"cocktailpeanut\",\n",
    "                    \"stars\": 0,\n",
    "                    \"has_install_js\": True,\n",
    "                    \"has_install_json\": False,\n",
    "                    \"has_pinokio_js\": True,\n",
    "                    \"installer_type\": \"js\",\n",
    "                    \"is_pinokio_app\": True,\n",
    "                    \"source\": \"official_appdata\"\n",
    "                }\n",
    "            }\n",
    "        \n",
    "        return apps_data\n",
    "    \n",
    "    def extract_categories(self):\n",
    "        \"\"\"Extract unique categories from apps data.\"\"\"\n",
    "        categories = set()\n",
    "        for app_data in self.apps_data.values():\n",
    "            if isinstance(app_data, dict):\n",
    "                category = app_data.get('category', 'Unknown')\n",
    "                categories.add(category)\n",
    "        return sorted(list(categories))\n",
    "    \n",
    "    def initialize_phase_systems(self):\n",
    "        \"\"\"Initialize all 12 phase systems.\"\"\"\n",
    "        \n",
    "        print(\"üîß Initializing phase systems...\")\n",
    "        \n",
    "        # Add repository path to Python path\n",
    "        github_repo_path = os.path.join(self.repo_path, 'github_repo')\n",
    "        if github_repo_path not in sys.path:\n",
    "            sys.path.insert(0, github_repo_path)\n",
    "        \n",
    "        # Initialize phase managers\n",
    "        self.phase_managers = {}\n",
    "        \n",
    "        # Phase 1: Cloud Detection\n",
    "        try:\n",
    "            from cloud_detection.cloud_detector import CloudDetector\n",
    "            from cloud_detection.repo_cloner import RepositoryCloner\n",
    "            self.phase_managers['cloud_detection'] = {\n",
    "                'detector': CloudDetector(),\n",
    "                'cloner': RepositoryCloner()\n",
    "            }\n",
    "            print(\"  ‚úÖ Phase 1: Cloud Detection initialized\")\n",
    "        except Exception as e:\n",
    "            print(f\"  ‚ö†Ô∏è Phase 1 initialization failed: {e}\")\n",
    "        \n",
    "        # Phase 2: Environment Management\n",
    "        try:\n",
    "            from environment_management.venv_manager import VirtualEnvironmentManager\n",
    "            from environment_management.shell_runner import ShellRunner\n",
    "            from environment_management.variable_system import VariableSystem\n",
    "            self.phase_managers['environment_management'] = {\n",
    "                'venv_manager': VirtualEnvironmentManager(),\n",
    "                'shell_runner': ShellRunner(),\n",
    "                'variable_system': VariableSystem()\n",
    "            }\n",
    "            print(\"  ‚úÖ Phase 2: Environment Management initialized\")\n",
    "        except Exception as e:\n",
    "            print(f\"  ‚ö†Ô∏è Phase 2 initialization failed: {e}\")\n",
    "        \n",
    "        # Phase 3: App Analysis\n",
    "        try:\n",
    "            from app_analysis.app_analyzer import AppAnalyzer\n",
    "            self.phase_managers['app_analysis'] = {\n",
    "                'analyzer': AppAnalyzer()\n",
    "            }\n",
    "            print(\"  ‚úÖ Phase 3: App Analysis initialized\")\n",
    "        except Exception as e:\n",
    "            print(f\"  ‚ö†Ô∏è Phase 3 initialization failed: {e}\")\n",
    "        \n",
    "        # Phase 4: Dependencies\n",
    "        try:\n",
    "            from dependencies.dependency_finder import DependencyFinder\n",
    "            from dependencies.pip_manager import PipManager\n",
    "            from dependencies.conda_manager import CondaManager\n",
    "            self.phase_managers['dependencies'] = {\n",
    "                'finder': DependencyFinder(),\n",
